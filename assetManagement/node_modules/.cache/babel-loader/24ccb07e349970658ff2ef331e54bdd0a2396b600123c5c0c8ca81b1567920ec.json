 tableData = [{\n        id: 1,\n        label: '论元 ',\n        value: '论元角色 '\n      }, {\n        id: 2,\n        label: this.productCompany,\n        value: this.value === 1 ? this.EventTypeTableData[0].company : this.value === 0 ? '' : this.EventTypeTableData[1].company\n      }, {\n        id: 3,\n        label: this.Product,\n        value: this.value === 1 ? this.EventTypeTableData[0].product : this.value === 0 ? '' : this.EventTypeTableData[1].product\n      }, {\n        id: 4,\n        label: this.Time,\n        value: this.value === 1 ? this.EventTypeTableData[0].time : this.value === 0 ? '' : this.EventTypeTableData[1].time\n      }, {\n        id: 5,\n        label: this.Range,\n        value: this.value === 1 ? this.EventTypeTableData[0].range : this.value === 0 ? '' : this.EventTypeTableData[1].range\n      }, {\n        id: 6,\n        label: this.Type,\n        value: this.value === 1 ? this.EventTypeTableData[0].type : this.value === 0 ? '' : this.EventTypeTableData[1].type\n      }];\n      return tableData;\n    }\n  }\n};","map":{"version":3,"names":["methods","lookDetail2","$router","push","path","query","QueryTableData","element","QueryInputContent","inputContent","dataFrom","eventType","lookDetail","value","productCompany","Time","Product","Range","console","log","Type","triggerClickEvent","components","props","data","EventTypeTableData","company","product","time","range","type","title","Company","companyValue","watch","newValue","computed","computedTableData","tableData","id","label"],"sources":["src/components/table2.vue"],"sourcesContent":["<template>\r\n    <div class=\"Table\">\r\n     <div class=\"title4\">{{ this.title }}</div>\r\n\r\n      <div class=\"header\" align=\"center\">\r\n        <div class=\"text\" align=\"left\">\r\n          {{ text }}\r\n        </div>\r\n        <el-button  v-if=\"false\" type=\"primary\" @click=\"lookDetail\"  class=\"detailButton\">查看详情</el-button>\r\n        <el-button   type=\"primary\" @click=\"loo