品',time:'获批时间',range:'获批产品作用范围',type:'获批产品类别'},\r\n          { company: '调价产品所在公司',product: '调价产品',time:'调价时间',range:'调价产品作用范围',type:'调价产品类别'},\r\n          ],\r\n\r\n\r\n          title:'抽取结果',\r\n          Company:'',\r\n          Product:'',\r\n          Time:'',\r\n          Range:'',\r\n          Type:'',\r\n          companyValue: '', // 在data中声明companyValue\r\n          value: 0, // 假设value的初始值为1\r\n        };\r\n      },\r\n\r\n      watch: {\r\n    value(newValue) {\r\n      if (newValue === 1) {\r\n        this.companyValue = this.EventTypeTableData[0].company;\r\n      } else if (newValue === 0) {\r\n        this.companyValue = this.EventTypeTableData[1].company;\r\n      }\r\n    },\r\n  },\r\n    \r\n      computed: {\r\n   \r\n        computedTableData() {\r\n      // 根据 value 来动态生成 tableData\r\n      const tableData = [\r\n        { id: 1, label: '论元 ', value: '论元角色 ' },\r\n        { id: 2, label: this.productCompany, \r\n        value: \r\n        this.value === 1 ? this.EventTypeTableData[0].company : (this.value === 0 ? '' : this.EventTypeTableData[1].company)\r\n      },\r\n        { id: 3, label: this.Product, value: \r\n           this.value === 1 ? this.EventTypeTableData[0].product : (this.value === 0 ? '' : this.EventTypeTableData[1].product) },\r\n\r\n        { id: 4, label: this.Time, value:  \r\n          this.value === 1 ? this.EventTypeTableData[0].time : (this.value === 0 ? '' : this.EventTypeTableData[1].time)},\r\n\r\n        { id: 5, label: this.Range, value:  \r\n          this.value === 1 ? this.EventTypeTableData[0].range : (this.value === 0 ? '' : this.EventTypeTableData[1].range) },\r\n\r\n        { id: 6, label: this.Type, value:  \r\n          this.value === 1 ? this.EventTypeTableData[0].type : (this.value === 0 ? '' : this.EventTypeTableData[1].type)},\r\n      ];\r\n\r\n      return tableData;\r\n    },\r\n      \r\n\r\n      },\r\n    };\r\n    \r\n    </script>\r\n    \r\n    \r\n    <style lang=\"less\">\r\n    //控制表格\r\