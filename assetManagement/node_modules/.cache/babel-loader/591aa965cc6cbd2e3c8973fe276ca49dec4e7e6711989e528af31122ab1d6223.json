{"ast":null,"code":"import Aside from \"../components/Aside2.vue\";\nimport Topbar from \"../components/Topbar.vue\";\nimport Navbar from \"../components/Navbar.vue\";\nimport BillContainer from \"../components/BillContainer.vue\";\nimport { amountToArray } from '../compute/amountFormat';\nimport { sumCount } from '../compute/amountFormat';\nexport default {\n  components: {\n    Aside,\n    Topbar,\n    Navbar,\n    BillContainer\n  },\n  data() {\n    return {\n      sortord: 'day',\n      items: [{\n        id: 1,\n        type: 'expend',\n        amount: 100.32,\n        date: '2023-10-01',\n        category: 'food',\n        remark: 'Lunch'\n      }, {\n        id: 2,\n        type: 'income',\n        amount: 200.21,\n        date: '2023-10-02',\n        category: 'food'\n      }, {\n        id: 3,\n        type: 'expend',\n        amount: 50,\n        date: '2023-10-03',\n        category: 'transport',\n        remark: 'Bus fare'\n      }, {\n        id: 4,\n        type: 'expend',\n        amount: 100.32,\n        date: '2023-10-01',\n        category: 'food',\n        remark: 'Lunch'\n      }, {\n        id: 5,\n        type: 'income',\n        amount: 200.21,\n        date: '2023-10-02',\n        category: 'food'\n      }, {\n        id: 6,\n        type: 'expend',\n        amount: 50,\n        date: '2023-10-03',\n        category: 'transport',\n        remark: 'Bus fare'\n      }\n      // 添加其他项目\n      ]\n    };\n  },\n\n  computed: {\n    sum() {\n      //支出收入求和\n      return sumCount(this.items);\n    },\n    income() {\n      //得到整数小数\n      return amountToArray(this.sum.income);\n    },\n    expend() {\n      //得到整数小数\n      return amountToArray(this.sum.expend);\n    }\n  }\n};","map":{"version":3,"names":["Aside","Topbar","Navbar","BillContainer","amountToArray","sumCount","components","data","sortord","items","id","type","amount","date","category","remark","computed","sum","income","expend"],"sources":["src/views/add.vue"],"sourcesContent":["\n<template>\n  <div class = box>\n    <el-aside width=\"200px\" class=\"aside\"  >\n      <Aside></Aside>\n    </el-aside>\n    <el-main >\n        <div class=\"login-input\">\n            <el-form :model=\"ruleForm\" status-icon :rules=\"rules\" ref=\"ruleForm\"  class=\"demo-ruleForm\">\n                 <el-form-item label=\"\" >\n                 <el-input  placeholder=\"邮箱\"></el-input>\n                 </el-form-item>\n                 <el-form-item label=\"\" >\n                     <el-input type=\"password\"  placeholder=\"密码\" clearable></el-input>\n                 </el-form-item> \n                 <el-form-item label=\"\" >\n                     <el-input type=\"password\"  placeholder=\"确认密码\" clearable></el-input>\n                 </el-form-item>\n                 <el-form-item>\n                     <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">\n                         <loading v-if=\"load\"></loading>\n                         <span v-else>注册</span>\n                     </el-button>\n                 </el-form-item>\n             </el-form>\n             <p class='register'><router-link to='/login'>已有账号,去登录</router-link></p>\n        </div>\n        \n  \n  </el-main>\n  </div>\n  \n</template>\n\n\n<script>\nimport Aside from \"../components/Aside2.vue\";\nimport Topbar from \"../components/Topbar.vue\";\nimport Navbar from \"../components/Navbar.vue\";\nimport BillContainer from \"../components/BillContainer.vue\";\nimport { amountToArray } from '../compute/amountFormat'\nimport { sumCount } from '../compute/amountFormat'\n\nexport default {\ncomponents: {\n  Aside,\n  Topbar,\n  Navbar,\n  BillContainer\n},\ndata(){\n  return{\n    sortord:'day',\n    items: [\n      { id: 1, type: 'expend', amount: 100.32, date: '2023-10-01', category: 'food', remark: 'Lunch' },\n      { id: 2, type: 'income', amount: 200.21, date: '2023-10-02', category: 'food' },\n      { id: 3, type: 'expend', amount: 50, date: '2023-10-03', category: 'transport', remark: 'Bus fare' },\n      { id: 4, type: 'expend', amount: 100.32, date: '2023-10-01', category: 'food', remark: 'Lunch' },\n      { id: 5, type: 'income', amount: 200.21, date: '2023-10-02', category: 'food' },\n      { id: 6, type: 'expend', amount: 50, date: '2023-10-03', category: 'transport', remark: 'Bus fare' },\n      // 添加其他项目\n    ]\n\n  }\n},\ncomputed: {\n  sum() {//支出收入求和\n    return sumCount(this.items)\n  },\n  income() {//得到整数小数\n  return amountToArray(this.sum.income)\n},\nexpend() {//得到整数小数\n  return amountToArray(this.sum.expend)\n},\n},\n};\n</script>\n<!--\n<style lang=\"less\">\n.box{\n  background: linear-gradient(#429691,#2A7C76);\n  background-size: 100% 15rem;\n  background-repeat: no-repeat;\n  padding-bottom: 2rem;\n  box-sizing: border-box;\n  display: flex;\n}\n.bill {\n  padding: 1rem;\n  color:#fff;\n\n  .count-card{\n    background-color: #429691;\n    padding: 1rem;\n    margin-top: 2rem;\n    border-radius: .4rem;\n    font-size: 14px;\n    box-shadow: 0 2px 10px #429691;\n    border: 1px solid rgb(107, 151, 134);\n    .item-container{\n      display: flex;\n      justify-content: space-between;\n      color: gray;\n      padding: 1rem;\n      padding-bottom: 0;\n      .item{\n        .amount{\n          color: #fff;\n          font-size: 14px;\n          span:nth-child(2){\n            font-size: 14px;\n          }\n        }\n      }\n    }\n  }\n  .bill-container{\n    margin-top: 2rem;\n    .select{\n      color: gray-1;\n      font-size: 14px;\n      margin: .6rem 0;\n      color: gray;\n      button{\n        width: 3rem;\n      }\n      .active{\n        font-weight: 600;\n        font-size: 14px;\n        color: 14px;\n      }\n    }\n  }\n}\n\n.aside{\n  background-color: #545c64;\n}\n</style>\n-->\n\n<style lang=\"less\" scoped>\n@import '../styles/common.less';\n@import '../styles/common.css';\n@import '../styles/main.css';\n.box{\nbackground: linear-gradient(#429691,#2A7C76);\nbackground-size: 100% 15rem;\nback