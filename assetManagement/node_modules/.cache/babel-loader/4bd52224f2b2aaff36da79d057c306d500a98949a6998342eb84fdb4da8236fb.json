\"移动\"+left);\r\n        console.log(\"移动\"+this.tabLen);\r\n        console.log(\"移动\"+100 / this.tabLen);\r\n        console.log(\"移动\"+this.selectedItem * 2 + 1); \r\n        console.log(\"移动\"+this.slideLeft)\r\n        },\r\n            // 刚进入页面没有点击时的下划线设置\r\n      slideLineLeft () {\r\n        console.log(\"我出来了\");\r\n       // this.tabLen = this.menuItems.length\r\n       // const left = 100 / this.tabLen / 2\r\n      // const left = 30 / this.tabLen / 2\r\n      //  this.slideLeft = left * (this.selectedItem * 2 + 1) + '%'\r\n      this.slideLeft = 2.7 + '%'\r\n        console.log(\"开始\"+ this.slideLeft);\r\n      },\r\n\r\n\r\n      handleSearch() {\r\n      // 在这里可以执行你想要的操作\r\n    }\r\n      },\r\n    \r\n        data() {\r\n    return {\r\n      selectedValue: false ,// 初始选中的值\r\n      selectedValue2: false ,// 初始选中的值\r\n      tabLen: '',\r\n   \r\n        slideLeft: '',\r\n\r\n        agreeShow: false,\r\n        agreeShow2: false,\r\n        selectedItem: null,\r\n        label:'公司名称：',\r\n        placeholder:'请输入公司名称',\r\n        selectedButton: null,\r\n        menuItems: [\r\n          { name: '公司', content: '菜单项 1 的内容。' },\r\n          { name: '股东', content: '菜单项 2 的内容。' },\r\n          { name: '产品', content: '菜单项 3 的内容。' },\r\n          { name: '事件类型', content: '菜单项 3 的内容。' }\r\n        ],\r\n      };\r\n\r\n\r\n    },\r\n\r\n\r\n\r\n    }\r\n    </script>\r\n    \r\n    \r\n    <style lang=\"less\">\r\n \r\n .search {\r\n    background-color: #ffffff;\r\n    position:fixed;\r\n    top:5%;\r\n    width:87%;\r\n    height: 25%;\r\n    margin-left:0px;\r\n    //margin-right: 5%;\r\n\r\n }\r\n\r\n .menu2{\r\n      display: flex;\r\n      background-color: #ffffff;\r\n      color: rgb(0, 0, 0);\r\n      padding: 10px;\r\n      border-bottom: 1px solid #b5b3b3;\r\n    }\r\n\r\n    .menu-item2 {\r\n      cursor: pointer;\r\n      margin: 0 10px;\r\n    }\r\n\r\n\r\n\r\n    .selected {\r\n  color: #0d4ae4; /* 选中颜色 */\r\n\r\n}\r\n\r\n.selected::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  bottom: -2px;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 2px;\r\n//  background-color: dodgerblue; /* 下方线条颜色 *///不对劲\r\n}\r\n\r\n    /* 样式用于内容显示区域 */\r\n    .content {\r\n      padding: 20px;\r\n    }\r\n\r\n    .product-container {\r\n      display: flex;\r\n      align-items: center;\r\n      margin-bottom: 30px;\r\n    \r\n    }\r\n\r\n    .eventType {\r\n      display: flex;\r\n      align-items: center;\r\n    //  margin-bottom: 40px;\r\n    \r\n    }\r\n\r\n\r\n    .product-label {\r\n      flex: 0.15;\r\n      //margin-right: 5px;\r\n      margin-left: 10%;\r\n      font-weight: bold;\r\n    }\r\n\r\n    .label2 {\r\n      margin-left: 13.5%;//和左边距离\r\n      font-weight: bold;\r\n      margin-right: 30px;//和按钮距离\r\n    }\r\n\r\n    .sublabel {\r\n      margin-left: 10px;\r\n      margin-right: 50px;\r\n      font-weight: bold;\r\n    }\r\n\r\n    .product-input {\r\n      flex: 0.7;\r\n      padding: 5px;\r\n      border: 1px solid #b5b3b3;\r\n    }\r\n\r\n    .button {\r\n      display: inline-block;\r\n      background-color: #ffffff;\r\n      cursor: pointer;\r\n      position: relative;\r\n    width: 15px;\r\n    height: 15px;\r\n    border: 1px solid #000000 !important;\r\n    border-radius: 2px;\r\n    }\r\n\r\n    .item-name {\r\n  margin: 0px 25px; /* 设置 item.name 的间距 */\r\n  font-weight: bold;\r\n  \r\n}\r\n\r\n  .tick::after {\r\n    content: \" \";\r\n    position: absolute;\r\n    display: inline-block;\r\n    width: 10px;\r\n    height: 6px;\r\n    border-width: 0 0 2px 2px;\r\n    overflow: hidden;\r\n    border-color: #0cbaf4;\r\n    border-style: solid;\r\n    -webkit-transform: rotate(-50deg);\r\n    transform: rotate(-50deg);\r\n    left: 2px;\r\n    top: 3px;\r\n  }\r\n\r\n  .tick2::after {\r\n    content: \" \";\r\n    position: absolute;\r\n    display: inline-block;\r\n    width: 10px;\r\n    height: 6px;\r\n    border-width: 0 0 2px 2px;\r\n    overflow: hidden;\r\n    border-color: #0cbaf4;\r\n    border-style: solid;\r\n    -webkit-transform: rotate(-50deg);\r\n    transform: rotate(-50deg);\r\n    left: 2px;\r\n    top: 3px;\r\n  }\r\n\r\n  .searchButton {\r\n  position: absolute;\r\n  width:300px;\r\n  height:30px;\r\n  bottom: 30px;\r\n  right: 0;\r\n  //margin: 10px;\r\n  margin-right: 60px;\r\n // border: 1px outset blue; /* 边框颜色为蓝色 */\r\n  background-color: rgb(255, 255, 255);\r\n  //color: rgb(0, 0, 0);\r\n}\r\n\r\n\r\n\r\n\r\n.line{\r\n  position: absolute;\r\n  bottom: 85%;\r\n  width: 70px;\r\n  //margin-left: -.4rem;\r\n  transition: all 0.3s;\r\n  height:.20rem;\r\n // border-radius: .1rem;\r\n  background: #0d4ae4;\r\n }\r\n\r\n /*\r\n .line{\r\n  position: absolute;\r\n  bottom: 0;\r\n  width: 40px;\r\n  margin-left: -.4rem;\r\n  transition: all 0.3s;\r\n  height: .08rem;\r\n  border-radius: .04rem;\r\n  background: #b4df0a;\r\n }\r\n */\r\n .checkbox {\r\n  margin: 10px; /* 外边距，设置下边距 */\r\n  /* 或者使用内边距\r\n  padding-bottom: 10px;\r\n  */\r\n}\r\n\r\n.vxe-checkbox--label{\r\n  overflow:visible !important\r\n\r\n}\r\n</style>\r\n"],"mappings":"AAkFA;AACA;EAIAA,QAAA;IAEA,KAAAC,aAAA;IACA;EACA;;EAEAC,OAAA;IAEAC,QAAAC,KAAA;MAAA;MACA,IAAAA,KAAA,CAAAA,KAAA,UACA;QACA,SAAAC,cAAA,UACA,KAAAA,cAAA,SAAAA,cAAA;MACA;IACA;IACAC,QAAAF,KAAA;MACA,IAAAA,KAAA,CAAAA,KAAA,UACA;QACA,SAAAG,aAAA,UACA,KAAAA,aAAA,SAAAA,aAAA;MACA;IACA;IAEAC,aAAA;MACA,KAAAC,SAAA,SAAAA,SAAA;IACA;;IACAC,cAAA;MACA,KAAAC,UAAA,SAAAA,UAAA;IACA;;IAGAC,aAAAC,YAAA;MACA,KAAAC,cAAA,GAAAD,YAAA;MACA;IACA;;IACAE,cAAAC,KAAA;MACAC,OAAA,CAAAC,GAAA,MAAAC,SAAA,CAAAH,KAAA,EAAAI,IAAA;MACA,KAAAC,YAAA,GAAAL,KAAA;MACA,KAAAM,KAAA,QAAAH,SAAA,CAAAH,KAAA,EAAAI,IAAA;MACA,KAAAG,WAAA,gBAAAJ,SAAA,CAAAH,KAAA,EAAAI,IAAA;MAEA,MAAAI,IAAA,aAAAC,MAAA;MACA;MACA;MACA;MACA;MACA;MACA;MACA,KAAAC,SAAA,GAAAF,IAAA,IAAAR,KAAA;MACA;;MAEA,IAAAA,KAAA;QACA,KAAAU,SAAA;MACA;MACA,IAAAV,KAAA;QACA,KAAAU,SAAA;MACA;MACA,IAAAV,KAAA;QACA,KAAAU,SAAA;MACA;MACA,IAAAV,KAAA;QACA,KAAAU,SAAA;MACA;MAGAT,OAAA,CAAAC,GAAA,QAAAM,IAAA;MACAP,OAAA,CAAAC