/svg>\n    </div>\n    <template   v-if=\"showType === 'stock'\">\n    <div class=\"ooo\">\n      <div class=\"info\">\n        <div class=\"name\">\n          <span>{{ this.data.corporate_name }}</span\n          ><!---<span style=\"color: rgb(162, 162, 167); font-weight: normal; margin-left: 10px\">{{ data.remark || this.label }}</span>-->\n        </div>\n        <div>{{ this.data.stock_code }}</div>\n      </div>\n      <div class=\"info\">\n        <div class=\"name\">\n          <span>{{ this.data.holdings }}</span\n          ><!---<span style=\"color: rgb(162, 162, 167); font-weight: normal; margin-left: 10px\">{{ data.remark || this.label }}</span>-->\n        </div>\n        <div>{{ this.data.holdings }}</div>\n      </div>\n      <div class=\"info\">\n        <div class=\"name\">\n          <span\n            :class=\"{\n              'red-text': data.stock_market_prices > data.buying_price,\n              'green-text': data.stock_market_prices <= data.buying_price,\n            }\"\n            >{{ data.stock_market_prices }}</span\n          >\n        </div>\n        <div>{{ data.buying_price }}</div>\n      </div>\n    </div>\n    </template>\n\n    <template v-else> \n      <div class=\"ooo\">\n      <div class=\"info\">\n        <div class=\"name\">\n          <span>{{ this.data.fund_name }}</span\n          ><!---<span style=\"color: rgb(162, 162, 167); font-weight: normal; margin-left: 10px\">{{ data.remark || this.label }}</span>-->\n        </div>\n        <div>{{ this.data.funds_code }}</div>\n      </div>\n      <div class=\"info\">\n        <div class=\"name\">\n          <span>{{ this.data.buying_price }}.00</span\n          ><!---<span style=\"color: rgb(162, 162, 167); font-weight: normal; margin-left: 10px\">{{ data.remark || this.label }}</span>-->\n        </div>\n      </div>\n      <div class=\"info\">\n        <div class=\"name\">\n          <span\n            >{{ data.fund_market_prices }}.00</span\n          >\n        </div>\n      </div>\n    </div>\n      </template>\n\n  </div>\n</template>\n<script>\nimport dayjs from \"dayjs\";\nimport { categoryes } from \"../types/types\"; //只导入特定的属性，变量\nimport { amountToArray } from \"../compute/amountFormat\";\nexport default {\n  data() {\n    return {\n      type: categoryes,\n      label: \"(无备注)\",\n    };\n  },\n  mounted() {\n    console.log(\"啊啊啊\", categoryes);\n    console.log(\"这里\", this.data.amount);\n  },\n  props: {\n    data: Object,\n    billtype: String,\n    showType:String,\n  },\n\n  computed: {\n    amount() {\n      return amountToArray(this.data.amount);\n    },\n  },\n  methods: {\n    formatDate(date) {\n      return dayjs(date).format(\"M-D\");\n    },\n    look() {\n      this.$emit(\"lookBill\", this.data);\n    },\n  },\n};\n</script>\n<!--\n<style scoped lang=\"less\">\n@import '../styles/common.less';\n.bill-item {\n  display: flex;\n  align-items: center;\n  padding: 0.5rem;\n  gap: 0.8rem;\n  .icon {\n    border-radius: 0.5rem;\n    .icon-font {\n      font-size: 2rem;\n    }\n  }\n  .info {\n    flex: 1;\n    color: red;\n    font-size: 14px;\n    .name {\n      font-size: 14px;\n      color: gray;\n      font-weight: 600;\n    }\n  }\n  .amount {\n    font-size: 14px;\n    color: red;\n    .decimal {\n      font-size: 14px;\n    }\n  }\n}\n</style>\n-->\n\n<style scoped lang=\"less\">\n@import \"../styles/common.less\";\n.bill-item {\n  display: flex;\n  align-items: center;\n  padding: 0.5rem;\n  gap: 0.8rem;\n  cursor: pointer;\n  .icon {\n    // margin: .5rem;\n    // width: 2rem;\n    // height: 2rem;\n    border: 0.5rem;\n    background-color: @primary-8;\n    border-radius: 0.5rem;\n    .iconfont {\n      font-size: 2rem;\n    }\n  }\n  .info {\n    color: @gray-1;\n    font-size: @font-size-sm;\n    .name {\n      width: 140px;\n      font-size: @font-size-base;\n      color: @gray-10;\n      font-weight: 600;\n    }\n  }\n  .red-text {\n    color: red;\n  }\n\n  .green-text {\n    color: green;\n  }\n\n  .amount {\n    font-size: @font-size-lg;\n    color: @primary-0;\n    .decimal {\n      font-size: @font-size-sm;\n    }\n  }\n  .ooo {\n    display: flex;\n    width: 100%;\n    //margin-left: 3rem;\n    gap: 0.5rem;\n    align-items: flex-end;\n    justify-content: space-between;\n    font-size: 0.8rem;\n    color: #000;\n    font-size: 1rem;\n    color: #000;\n    font-weight: bold;\n  }\n}\n</style>\n"],"mappings":"AA4FA,OAAAA,KAAA;AACA,SAAAC,UAAA;AACA,SAAAC,aAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAH,UAAA;MACAI,KAAA;IACA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,QAAAP,UAAA;IACAM,OAAA,CAAAC,GAAA,YAAAL,IAAA,CAAAM,MAAA;EACA;EACAC,KAAA;IACAP,IAAA,EAAAQ,MAAA;IACAC,QAAA,EAAAC,MAAA;IACAC,QAAA,EAAAD;EACA;EAEAE,QAAA;IACAN,OAAA;MACA,OAAAP,aAAA,MAAAC,IAAA,CAAAM,MAAA;IACA;EACA;EACAO,OAAA;IACAC,WAAAC,IAAA;MACA,OAAAlB,KAAA,CAAAkB,IAAA,EAAAC,MAAA;IACA;IACAC,KAAA;MACA,KAAAC,KAAA,kBAAAlB,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}