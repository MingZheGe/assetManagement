{"ast":null,"code":"import dayjs from 'dayjs';\nimport BillItem from './BillItem.vue'; // Import your BillItem component\nimport { sumCount } from '../compute/amountFormat';\nexport default {\n  components: {\n    BillItem\n  },\n  data() {\n    return {\n      week: ['星期日', '星期一', '星期二', '星期三', '星期四', '星期五', '星期六'],\n      income: '',\n      expend: ''\n    };\n  },\n  props: {\n    month: String,\n    data: Array,\n    amountCount: Object\n  },\n  mounted() {\n    if (this.amountCount) {\n      this.income = this.amountCount.income;\n      this.expend = this.amountCount.expend;\n    }\n    if (this.month) {\n      console.log(\"hahahahah\", this.month);\n      console.log(\"hahahahah\", this.data);\n    }\n  },\n  watch: {\n    amountCount(newValue, oldValue) {\n      console.log(\"收到ampountCount\", newValue);\n    }\n  },\n  methods: {\n    getBill(data) {\n      this.$emit('getBill', data);\n      console.log(\"container收到为\", data);\n    }\n  },\n  computed: {\n    today() {\n      return dayjs().format('M月D日 ' + this.week[dayjs().day()]);\n    },\n    containerStyle() {\n      // 根据 month 的值返回不同的样式\n      if (this.month) {\n        return {\n          // 样式属性\n        };\n      } else {\n        // 可以定义其他 month 对应的样式\n        return {\n          height: '300px',\n          width: '100%',\n          overflowY: 'auto',\n          marginTop: '20px'\n\n          // 其他样式属性\n        };\n      }\n    }\n  }\n};","map":{"version":3,"names":["dayjs","BillItem","sumCount","components","data","week","income","expend","props","month","String","Array","amountCount","Object","mounted","console","log","watch","newValue","oldValue","methods","getBill","$emit","computed","today","format","day","containerStyle","height","width","overflowY","marginTop"],"sources":["src/components/BillContainer.vue"],"sourcesContent":["<template>\n  <div class=\"bill-list\">\n    <div class=\"info-bar\">\n      <div style=\"font-size: 1rem; color: #000; font-weight: bold;\">\n        {{ month !== 'loan' ? (month ? month + '月' : '今天') : '' }}\n      </div>\n      <div style=\"flex: 1\">\n        {{ month ? '' : today }}\n      </div>\n      <div class=\"info\" style=\"font-size: 1rem; color: #000;\">\n        <span>收</span>\n        <span class=\"amount\">{{ this.amountCount.income }}</span>\n        <span>支</span>\n        <span class=\"amount\">{{ this.amountCount.expend }}</span>\n      </div>\n    </div>\n\n    <div  :style=\"containerStyle\">\n      <BillItem v-for=\"item in data\" :key=\"item.id\" :data=\"item\"  @lookBill=\"getBill\" :billtype=\"'bill'\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport dayjs from 'dayjs'\nimport BillItem from './BillItem.vue' // Import your BillItem component\nimport {sumCount} from '../compute/amountFormat'\n\nexport default {\n  components: {\n    BillItem,\n  },\n  data() {\n    return {\n      week: ['星期日', '星期一', '星期二', '星期三', '星期四', '星期五', '星期六'],\n      income:'',\n      expend:''\n    }\n  },\n  props: {\n    month: String,\n    data:Array,\n    amountCount:Object,\n\n  },\n  mounted(){\n    if (this.amountCount) {\n    this.income = this.amountCount.income;\n    this.expend = this.amountCount.expend;\n  }\n  if (this.month) {\n    console.log(\"hahahahah\",this.month)\n    console.log(\"hahahahah\",this.data)\n  }\n  \n\n  },\n  watch:{\n    amountCount(newValue,oldValue){\n      console.log(\"收到ampountCount\",newValue)\n\n    }\n\n  },\n  methods:{\n    \n    getBill(data){\n      this.$emit('getBill',data)\n      console.log(\"container收到为\",data)\n\n    },\n  },\n  computed: {\n    today() {\n      return dayjs().format('M月D日 ' + this.week[dayjs().day()])\n    },\n    containerStyle() {\n      // 根据 month 的值返回不同的样式\n      if (this.month) {\n        return {\n          // 样式属性\n        };\n      } else {\n        // 可以定义其他 month 对应的样式\n        return {\n          height: '300px',\n          width: '100%',\n          overflowY: 'auto',\n       \n          marginTop:'20px'\n          \n          // 其他样式属性\n        };\n      \n      }\n    },\n   \n  },\n \n}\n</script>\n\n<style scoped lang=\"less\">\n@import '../styles/common.less';\n.bill-list{\n  margin-top: .5rem;\n  color: gray;\n  padding: 1rem;\n  background-color: #fff;\n\n  .info-bar{\n  display: flex;\n  gap: .5rem;\n  align-items: flex-end;\n  font-size: 0.8rem;\n  color: #000;\n  .amount{\n    color:@gray-1;\n    margin: 0 .5rem 0 .2rem;\n  }\n}\n}\n</style>"],"mappings":"AAwBA,OAAAA,KAAA;AACA,OAAAC,QAAA;AACA,SAAAC,QAAA;AAEA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;MACAC,MAAA;IACA;EACA;EACAC,KAAA;IACAC,KAAA,EAAAC,MAAA;IACAN,IAAA,EAAAO,KAAA;IACAC,WAAA,EAAAC;EAEA;EACAC,QAAA;IACA,SAAAF,WAAA;MACA,KAAAN,MAAA,QAAAM,WAAA,CAAAN,MAAA;MACA,KAAAC,MAAA,QAAAK,WAAA,CAAAL,MAAA;IACA;IACA,SAAAE,KAAA;MACAM,OAAA,CAAAC,GAAA,mBAAAP,KAAA;MACAM,OAAA,CAAAC,GAAA,mBAAAZ,IAAA;IACA;EAGA;EACAa,KAAA;IACAL,YAAAM,QAAA,EAAAC,QAAA;MACAJ,OAAA,CAAAC,GAAA,mBAAAE,QAAA;IAEA;EAEA;EACAE,OAAA;IAEAC,QAAAjB,IAAA;MACA,KAAAkB,KAAA,YAAAlB,IAAA;MACAW,OAAA,CAAAC,GAAA,iBAAAZ,IAAA;IAEA;EACA;EACAmB,QAAA;IACAC,MAAA;MACA,OAAAxB,KAAA,GAAAyB,MAAA,gBAAApB,IAAA,CAAAL,KAAA,GAAA0B,GAAA;IACA;IACAC,eAAA;MACA;MACA,SAAAlB,KAAA;QACA;UACA;QAAA,CACA;MACA;QACA;QACA;UACAmB,MAAA;UACAC,KAAA;UACAC,SAAA;UAEAC,SAAA;;UAEA;QACA;MAEA;IACA;EAEA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}