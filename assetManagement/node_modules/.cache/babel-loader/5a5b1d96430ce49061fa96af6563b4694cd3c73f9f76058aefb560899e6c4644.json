{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import '@fortawesome/fontawesome-free/css';\nimport axios from \"axios\";\nexport default {\n  mounted() {\n    this.slideLineLeft();\n    // 如果不写这个函数，页面刚加载时默认tab的下划线位置是不对的\n  },\n\n  methods: {\n    querySearch(queryString, cb) {\n      console.log(queryString);\n      this.remindData = [];\n      if (this.searchProduct !== \"\") {\n        axios.post(\"http://39.101.141.53:8086/SelectByProductNameFuzzyQuery\", {\n          fuzzy: queryString\n        }).then(response => {\n          //    this.isLoading = true;\n          console.log(\"finish\");\n          this.getData = response.data;\n          this.getData.forEach(item => {\n            let obj = {\n              value: \"\"\n            };\n            obj.value = item;\n            this.remindData.push(obj);\n          });\n        }).catch(function (error) {\n          console.log(error);\n        });\n        let nameTipsArray = this.remindData;\n        // 调用 callback 返回建议列表的数据\n        cb(nameTipsArray);\n      }\n    },\n    change1(value) {\n      //单选逻辑\n      if (value.value == true) {\n        if (this.selectedValue2 == true) this.selectedValue2 = !this.selectedValue2;\n      }\n    },\n    change2(value) {\n      if (value.value == true) {\n        if (this.selectedValue == true) this.selectedValue = !this.selectedValue;\n      }\n    },\n    getAgreeShow() {\n      this.agreeShow = !this.agreeShow; // 控制点击\n    },\n\n    getAgreeShow2() {\n      this.agreeShow2 = !this.agreeShow2; // 控制点击\n    },\n\n    selectButton(buttonNumber) {\n      this.selectedButton = buttonNumber;\n      // '.menu2'.borderColor = '#ffffff'//不好使\n    },\n\n    changeContent(index) {\n      console.log(this.menuItems[index].name);\n      this.selectedItem = index;\n      this.label = this.menuItems[index].name + \"名称：\";\n      this.placeholder = \"请输入\" + this.menuItems[index].name + \"名称\";\n      const left = 30 / this.tabLen / 2;\n      // const left = 1\n      // 一次滑动二分一tab标签值\n      // 三个标签，可以看成把屏幕分成了6份，每人占2份\n      // 第一个下划线，从第1份的地方开始，是0*2+1\n      // 第二个下划线，从第3份的地方开始，是1*2+1\n      // 第三个下划线，从第5份的地方开始, 是2*2+1\n      this.slideLeft = left * (index * 2 + 1) + '%';\n      //  this.slideLeft = 0+'%';\n\n      if (index === 0) {\n        this.slideLeft = 1.7 + '%';\n      }\n      if (index === 1) {\n        this.slideLeft = 8.4 + '%';\n      }\n      if (index === 2) {\n        this.slideLeft = 15.2 + '%';\n      }\n      if (index === 3) {\n        this.slideLeft = 22.8 + '%';\n      }\n      console.log(\"移动\" + left);\n      console.log(\"移动\" + this.tabLen);\n      console.log(\"移动\" + 100 / this.tabLen);\n      console.log(\"移动\" + this.selectedItem * 2 + 1);\n      console.log(\"移动\" + this.slideLeft);\n    },\n    // 刚进入页面没有点击时的下划线设置\n    slideLineLeft() {\n      console.log(\"我出来了\");\n      // this.tabLen = this.menuItems.length\n      // const left = 100 / this.tabLen / 2\n      // const left = 30 / this.tabLen / 2\n      //  this.slideLeft = left * (this.selectedItem * 2 + 1) + '%'\n      this.slideLeft = 1.7 + '%';\n      console.log(\"开始\" + this.slideLeft);\n    },\n    search() {\n      this.$emit('forScrollTable', this.searchProduct); //传给父组件\n      console.log(\"search组件\" + this.searchProduct);\n    }\n  },\n  data() {\n    return {\n      isLoading: false,\n      selectedValue: false,\n      // 初始选中的值\n      selectedValue2: false,\n      // 初始选中的值\n      tabLen: '',\n      searchProduct: '',\n      slideLeft: '',\n      agreeShow: false,\n      agreeShow2: false,\n      selectedItem: null,\n      label: '公司名称：',\n      placeholder: '请输入公司名称',\n      selectedButton: null,\n      menuItems: [{\n        name: '公司',\n        content: '菜单项 1 的内容。',\n        clickable: false\n      }, {\n        name: '股东',\n        content: '菜单项 2 的内容。',\n        clickable: false\n      }, {\n        name: '产品',\n        content: '菜单项 3 的内容。',\n        clickable: true\n      }, {\n        name: '事件类型',\n        content: '菜单项 4 的内容。',\n        clickable: false\n      }]\n    };\n  }\n};","map":{"version":3,"names":["axios","mounted","slideLineLeft","methods","querySearch","queryString","cb","console","log","remindData","searchProduct","post","fuzzy","then","response","getData","data","forEach","item","obj","value","push","catch","error","nameTipsArray","change1","selectedValue2","change2","selectedValue","getAgreeShow","agreeShow","getAgreeShow2","agreeShow2","selectButton","buttonNumber","selectedButton","changeContent","index","menuItems","name","selectedItem","label","placeholder","left","tabLen","slideLeft","search","$emit","isLoading","content","clickable"],"sources":["src/components/search.vue"],"sourcesContent":["<template>\r\n    <div class=\"search\">\r\n     \r\n        <div class=\"menu2\">\r\n         \r\n            <div class=\"menu-item2\" \r\n                 v-for=\"(item, index) in menuItems\" \r\n                :key=\"index\" \r\n                :class=\"{ 'selected': selectedItem === index }\"\r\n                @click=\"changeContent(index)\">\r\n                <span class=\"item-name\">\r\n              \r\n                  {{ item.name }}\r\n                 \r\n                </span>\r\n                <i class=\"line\" ></i>\r\n           </div>     \r\n       </div>\r\n      \r\n    <div class=\"content\" id=\"targetDiv\">\r\n\r\n        <div class=\"product-container\">\r\n    <label class=\"product-label\" for=\"product-name\">{{this.label}}</label>\r\n  <!--  <input class=\"product-input\" type=\"text\" id=\"product-name\" name=\"product-name\" v-model=\"searchProduct\" :placeholder=this.placeholder>-->\r\n    <el-autocomplete\r\n    class=\"product-input\"\r\n            v-model=\"searchProduct\"\r\n            :popper-append-to-body=\"false\"\r\n            :placeholder=this.placeholder\r\n            size=\"medium\"\r\n            label-width=400px\r\n            :fetch-suggestions=\"querySearch\"\r\n            @select=\"handleSelect\"\r\n            :loading=\"isLoading\"\r\n          ></el-autocomplete>\r\n  </div>\r\n  <div class=\"eventType\">\r\n    <label class=\"label2\">事件类型：</label>\r\n    <vxe-checkbox\r\n      v-model=\"selectedValue\"\r\n      name=\"radioGroup\"\r\n      class=\"checkbox\"\r\n      content=\"获批事件\"\r\n      @change=\"change1\"\r\n    ></vxe-checkbox>\r\n    \r\n    <vxe-checkbox\r\n      v-model=\"selectedValue2\"\r\n      name=\"radioGroup\"\r\n      class=\"checkbox\"\r\n      content=\"调价事件\"\r\n      @change=\"change2\"\r\n    ></vxe-checkbox>\r\n\r\n\r\n \r\n  </div>\r\n     \r\n\r\n  <div class=\"searchButton\">\r\n    \r\n    <el-button type=\"primary\" @click=\"search\">查询</el-button>\r\n </div>\r\n    </div>\r\n\r\n  \r\n    </div>\r\n    </template>\r\n    \r\n    <script>\r\n   // import '@fortawesome/fontawesome-free/css';\r\n   import axios from \"axios\";\r\n    export default {\r\n\r\n\r\n\r\n      mounted () {\r\n\r\n      this.slideLineLeft()\r\n      // 如果不写这个函数，页面刚加载时默认tab的下划线位置是不对的\r\n    },\r\n\r\n        methods: {\r\n\r\n          querySearch(queryString, cb) {\r\n      console.log(queryString)\r\n      this.remindData=[]\r\n      if (this.searchProduct !== \"\") {\r\n        axios\r\n        .post(\"http://39.101.141.53:8086/SelectByProductNameFuzzyQuery\", {\r\n          fuzzy: queryString\r\n        })\r\n        .then((response) => {\r\n      //    this.isLoading = true;\r\n          console.log(\"finish\")\r\n          this.getData = response.data; \r\n          this.getData.forEach((item) => { \r\n          let obj = { value: \"\"}; \r\n          obj.value = item; \r\n          this.remindData.push(obj);})\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n        let nameTipsArray = this.remindData;\r\n        // 调用 callback 返回建议列表的数据\r\n        cb(nameTipsArray);\r\n      }\r\n      \r\n    },\r\n\r\n         change1(value){//单选逻辑\r\n           if(value.value==true)\r\n           {\r\n             if(this.selectedValue2==true)\r\n             this.selectedValue2=!this.selectedValue2;\r\n           }\r\n          },\r\n            change2(value){\r\n               if(value.value==true)\r\n               {\r\n                   if(this.selectedValue==true)\r\n                   this.selectedValue=!this.selectedValue;  \r\n               }\r\n            },\r\n\r\n            getAgreeShow() {\r\n    this.agreeShow = !this.agreeShow; // 控制点击\r\n            },\r\n            getAgreeShow2() {\r\n    this.agreeShow2 = !this.agreeShow2; // 控制点击\r\n            },\r\n\r\n\r\n            selectButton(buttonNumber) {\r\n          this.selectedButton = buttonNumber;\r\n         // '.menu2'.borderColor = '#ffffff'//不好使\r\n        },\r\n        changeContent(index) {\r\n         console.log(this.menuItems[index].name);\r\n         this.selectedItem = index;\r\n         this.label=this.menuItems[index].name+\"名称：\";\r\n         this.placeholder=\"请输入\"+this.menuItems[index].name+\"名称\";\r\n\r\n         const left = 30 / this.tabLen / 2\r\n       // const left = 1\r\n        // 一次滑动二分一tab标签值\r\n        // 三个标签，可以看成把屏幕分成了6份，每人占2份\r\n        // 第一个下划线，从第1份的地方开始，是0*2+1\r\n        // 第二个下划线，从第3份的地方开始，是1*2+1\r\n        // 第三个下划线，从第5份的地方开始, 是2*2+1\r\n        this.slideLeft = left * (index * 2 + 1) + '%'\r\n     //  this.slideLeft = 0+'%';\r\n\r\n     if(index===0){\r\n      this.slideLeft = 1.7 + '%';\r\n     }\r\n     if(index===1){\r\n      this.slideLeft = 8.4 + '%';\r\n     }\r\n     if(index===2){\r\n      this.slideLeft = 15.2 + '%';\r\n     }\r\n     if(index===3){\r\n      this.slideLeft = 22.8 + '%';\r\n     }\r\n       \r\n\r\n        console.log(\"移动\"+left);\r\n        console.log(\"移动\"+this.tabLen);\r\n        console.log(\"移动\"+100 / this.tabLen);\r\n        console.log(\"移动\"+this.selectedItem * 2 + 1); \r\n        console.log(\"移动\"+this.slideLeft)\r\n        },\r\n            // 刚进入页面没有点击时的下划线设置\r\n      slideLineLeft () {\r\n        console.log(\"我出来了\");\r\n       // this.tabLen = this.menuItems.length\r\n       // const left = 100 / this.tabLen / 2\r\n      // const left = 30 / this.tabLen / 2\r\n      //  this.slideLeft = left * (this.selectedItem * 2 + 1) + '%'\r\n      this.slideLeft = 1.7 + '%'\r\n        console.log(\"开始\"+ this.slideLeft);\r\n      },\r\n\r\n    search(){\r\n      this.$emit('forScrollTable', this.searchProduct);//传给父组件\r\n      console.log(\"search组件\"+this.searchProduct)\r\n    },\r\n  \r\n      },\r\n    \r\n        data() {\r\n    return {\r\n      isLoading: false,\r\n      selectedValue: false ,// 初始选中的值\r\n      selectedValue2: false ,// 初始选中的值\r\n      tabLen: '',\r\n     searchProduct:'',\r\n        slideLeft: '',\r\n        agreeShow: false,\r\n        agreeShow2: false,\r\n        selectedItem: null,\r\n        label:'公司名称：',\r\n        placeholder:'请输入公司名称',\r\n        selectedButton: null,\r\n        menuItems: [\r\n        { name: '公司', content: '菜单项 1 的内容。', clickable: false },\r\n        { name: '股东', content: '菜单项 2 的内容。', clickable: false },\r\n        { name: '产品', content: '菜单项 3 的内容。', clickable: true },\r\n        { name: '事件类型', content: '菜单项 4 的内容。', clickable: false },\r\n      ],\r\n      };\r\n\r\n\r\n    },\r\n\r\n\r\n\r\n    }\r\n    </script>\r\n    \r\n    \r\n    <style lang=\"less\">\r\n \r\n .search {\r\n    background-color: #ffffff;\r\n    position:fixed;\r\n    top:5%;\r\n    width:87%;\r\n    height: 25%;\r\n    margin-left:0px;\r\n    //margin-right: 5%;\r\n\r\n }\r\n\r\n .menu2{\r\n  position: relative;\r\n      display: flex;\r\n      background-color: #ffffff;\r\n      color: rgb(0, 0, 0);\r\n      padding: 10px;\r\n      border-bottom: 1px solid #b5b3b3;\r\n    }\r\n\r\n    .menu-item2 {\r\n      cursor: pointer;\r\n      margin: 0 10px;\r\n    }\r\n\r\n    .menu-item2.disabled {\r\n  cursor: not-allowed;\r\n  /* 禁用时的样式，例如改为灰色 */\r\n}\r\n\r\n    .selected {\r\n  color: #0d4ae4; /* 选中颜色 */\r\n\r\n}\r\n\r\n.selected::after {\r\n  content: \"\";\r\n  po