Items() {\r\n          // 返回前三个元素\r\n          return this.tableData.slice(0, 2);\r\n        },\r\n        secondColumnItems() {\r\n          // 返回后三个元素\r\n          return this.tableData.slice(2,4);\r\n        },\r\n        computedTableData() {\r\n      // 根据 value 来动态生成 tableData\r\n      const tableData = [\r\n        { id: 1, label: '论元 ', value: '论元角色 ' },\r\n        { id: 2, label: this.value === 1 ? this.EventTypeTableData[0].company : '', value: this.companyValue },\r\n        { id: 3, label: this.Product, value: '' },\r\n        { id: 4, label: this.Time, value: '获批时间' },\r\n        { id: 5, label: this.Range, value: '获批产品作用范围' },\r\n        { id: 6, label: this.Type, value: '获批产品类别' },\r\n      ];\r\n\r\n      return tableData;\r\n    },\r\n      \r\n\r\n      },\r\n    };\r\n    \r\n    </script>\r\n    \r\n    \r\n    <style lang=\"less\">\r\n    //控制表格\r\n/*\r\n.custom-table {\r\n  font-size: 17px; \r\n  width: 100%; \r\n  //height:200px;\r\n  border-collapse: collapse; \r\n}\r\n\r\n\r\n.custom-table td {\r\n  border: 1px solid #ccc; \r\n  padding: 10px; \r\n}\r\n.custom-table td {\r\n  width: 150px; \r\n  height:80px;\r\n}\r\n*/\r\n\r\n     table {\r\n    width: 50%;\r\n  }\r\n  /*\r\n  th, td {\r\n    border: 1px solid #dddddd;\r\n    text-align: left;\r\n    padding: 8px;\r\n    width:50px;\r\n    height:50px;\r\n  }\r\n  */\r\n  .title4{\r\n  font-size: 15px;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  position: absolute;\r\n  top: 20px;\r\n  left: 18px;\r\n  color:#000000\r\n}\r\n  .custom-table {\r\n  //font-size: 17px; \r\n  //width: 50%; \r\n  height:30%;\r\n // top:40px;\r\n  margin-top:10px;\r\n  //height:200px;\r\n  border-collapse: collapse; \r\n  vertical-align: middle;\r\n}\r\n\r\n.custom-table td {\r\n  border: 1px solid #000000; \r\n  padding: 10px; \r\n}\r\n.custom-table td {//单元格长度\r\n  width: 100px; \r\n  height:40px;\r\n  vertical-align: middle;\r\n}\r\n  th {\r\n    background-color: #dddddd;\r\n    vertical-align: